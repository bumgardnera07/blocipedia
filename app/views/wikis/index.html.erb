<h1>All Wikis</h1>
<% @wikis.each do |l| %>
   <% if l.private != true || current_user.admin? || current_user.premium? %> 
    <div class="media">
     <div class="media-body">
       <h4 class="media-heading">
         <%= link_to l.title, l %>
       </h4>
     </div>
   </div>
  <% end %>
<% end %>

<div class="col-md-4">
     <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
</div>
